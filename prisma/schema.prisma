// This is the Prisma schema file, learn more about it at
// https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Article {
  id    String @id @default(cuid())
  slug  String @unique
  views Int    @default(0)
}
